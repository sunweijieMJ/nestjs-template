{
  "$schema": "https://raw.githubusercontent.com/streetsidesoftware/cspell/main/cspell.schema.json",
  "version": "0.2",
  "language": "en",
  "minWordLength": 3,
  "files": ["src/**/*.{js,ts}"],
  "dictionaries": ["typescript", "node", "npm", "softwareTerms"],
  "words": [
    "nestjs",
    "typeorm",
    "bcrypt",
    "bcryptjs",
    "dto",
    "dtos",
    "readonly",
    "nullable",
    "upsert",
    "seeders",
    "serializer",
    "middlewares",
    "authenticator",
    "datasource",
    "autoload",
    "autopopulate",
    "preload",
    "typecheck",
    "tsconfig",
    "transpile",
    "esmodule",
    "commonjs",
    "hygen",
    "commitlint",
    "husky",
    "prettierrc",
    "editorconfig",
    "eslintrc",
    "nodemailer",
    "handlebars",
    "presigner",
    "multer",
    "supertest",
    "subquery",
    "subqueries",
    "paginator",
    "paginate",
    "pagination",
    "unprocessable",
    "unauthorize",
    "signup",
    "signin",
    "logout",
    "auth",
    "authn",
    "authz",
    "oauth",
    "jwks",
    "jwtid",
    "refreshtoken",
    "accesstoken",
    "dotenv",
    "rimraf",
    "tslib",
    "rxjs",
    "swc",
    "sourcemap",
    "entrypoint",
    "postgres",
    "postgresql",
    "mongodb",
    "mongoose",
    "persistance",
    "relational",
    "boilerplate",
    "ioredis",
    "bullmq",
    "liveness",
    "readiness",
    "healthcheck",
    "healthchecks",
    "terminus",
    "Procfile",
    "wechat",
    "alipay",
    "jsapi",
    "openid",
    "unionid",
    "appid",
    "mchid",
    "prepay",
    "refundable",
    "outTradeNo",
    "transactionId"
  ],
  "patterns": [
    {
      "name": "jwt-token",
      "pattern": "/eyJ[A-Za-z0-9_-]+\\.[A-Za-z0-9_-]+\\.[A-Za-z0-9_-]+/g"
    },
    {
      "name": "import-path",
      "pattern": "/@\\/[a-zA-Z0-9/-]+/g"
    }
  ],
  "ignoreRegExpList": ["jwt-token", "import-path"],
  "ignorePaths": ["node_modules", "dist", "coverage", "*.lock", "*.json", ".git", "CHANGELOG.md"]
}
